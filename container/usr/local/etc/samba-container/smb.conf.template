[global]
	include = registry
	restrict anonymous = 2
	server smb encrypt = required
	server signing = mandatory
	load printers = no
	force user = nobody
	force group = nobody
	create mask = 0660
	directory mask = 0770
	guest ok = no
	browsable = yes
	writable = yes
	read only = no

