[global]
	include = registry
	server role = standalone server
	workgroup = WORKGROUP
	mdns name = mdns
	multicast dns register = no
	security = user
	passdb backend = tdbsam
	encrypt passwords = yes
	restrict anonymous = 2
	map to guest = never
	server min protocol = SMB2_02
	server smb encrypt = required
	server signing = mandatory
	load printers = no
